{
  "name": "assign",
  "version": "0.0.0-development",
  "description": "GitHub Action to assign reviewers when dependencies are changed using dependency-owners",
  "author": "Kirk Eaton <contact@kirkeaton.ca>",
  "bugs": {
    "url": "https://github.com/dependency-owners/assign/issues"
  },
  "dependencies": {
    "@actions/core": "2.0.1",
    "@actions/exec": "2.0.0",
    "@actions/github": "6.0.1",
    "dependency-owners": "3.0.2"
  },
  "devDependencies": {
    "@dependency-owners/package-json-loader": "2.0.0",
    "@kirkeaton/prettier-config": "1.0.5",
    "@kirkeaton/semantic-release-config": "2.0.0",
    "@kirkeaton/tsconfig": "2.1.0",
    "@types/node": "24.10.4",
    "esbuild": "0.27.2",
    "prettier": "3.7.4",
    "semantic-release": "25.0.2",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=24"
  },
  "exports": "./dist/index.js",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/dependency-owners/assign#readme",
  "keywords": [
    "actions",
    "react",
    "setup"
  ],
  "license": "BSD-3-Clause",
  "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dependency-owners/assign.git"
  },
  "scripts": {
    "build": "pnpm clean && ./build.mjs",
    "clean": "rm -rf dist",
    "format": "prettier \"**/*.{js,json,md,mjs,mts}\" --write",
    "lint": "prettier \"**/*.{js,json,md,mjs,mts}\" --check",
    "typecheck": "tsc --noEmit"
  },
  "type": "commonjs"
}
